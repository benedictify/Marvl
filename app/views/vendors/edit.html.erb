<center>
  <h1>Update <%=@vendor.name%>'s Information</h1>
</center>
<%= form_for @vendor, html: {class: "form-group"} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :state %>
  <%= f.text_field :state %><br>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %><br>
  <%= f.label :phone %>
  <%= f.text_field :phone %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :website %>
  <%= f.text_field :website %><br>

  <%= f.submit "Submit Vendor" %> 
<% end %>