<h1>CPA Member Institutions</h1>

<% @organisations.each do |organisation| %>
  <p><strong><%= organisation.name %></strong></p>
  <% if organisation.users.length > 0 %>
    <ul>
    	<% organisation.users.each do |user| %>
          <li><%= link_to user.full_name, user %></li>
        <% end %>
    </ul>
  <% end %>
<% end %>

