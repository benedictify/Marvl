<h1>Location Information</h1>

<h3><%= @location.name %></h3>
<%= @location.street_address %><br>
<%= @location.city %>, <%= @location.state %>, <%= @location.zip_code %><br>
<%= @location.phone %><br>

<h3>Affliated Members</h3>
  <ul>
    <%= @location.organisation.users do |user| %>
      <li>Representative: <%= link_to user.full_name , user %></li>
    <% end %>
  </ul>


